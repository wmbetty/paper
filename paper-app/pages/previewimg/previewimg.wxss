/* pages/preimg/previewimg.wxss */
page{background: #222;position: relative;}
.d-content{
    /* margin-top: 130rpx; */
    width: 100%;height: 100%
}
.swiper-item{width: 100%;height: 100%;overflow: hidden;background: #333;}
.sw-block{position: relative;width: 100%;height: 100%;}
.img-outer-conts{
  position: relative;width: 100%;height: 100%;overflow: hidden;display: flex;
  align-items: center;
  justify-content: center;
}
.showmian{width: 100%;height: 100%;white-space: nowrap; display: block}
.img-item{background: #f1f1f1;width: 100%;}
/* height: 100%; */
.cont-botom{
    position: fixed;bottom: 0;left: 0;width: 100%;background: rgba(0, 0, 0, 0.2);z-index: 99;
    display: flex;justify-content: space-between;align-items: center;
    height: 130rpx;
    /* padding: 26rpx 0; */
    color: #4E98F1;font-size: 30rpx;
}
.title-cont{
  display: flex;align-items: center;margin-left: 30rpx;
}
.paper-title{
  max-width: 290rpx;margin-right: 18rpx;
  overflow: hidden;white-space:nowrap;text-overflow:ellipsis;word-break: break-all;
}
.operate-btn-cont{
  /* margin-right: 40rpx; */
  display: flex;align-items: center;
}
.operate-btn{
  width: 84rpx;height: 84rpx;margin-right: 40rpx;
}
.view-img-box{
  position: fixed;width: 100%;height: 100%;background: #222;
  top: 0;left: 0;z-index: 1000;
  display: flex;justify-content: center;
  align-items: center;
}
.view-img-box image{width: 100%;}
/* height: 100%; */
.down-tip{
  position: fixed;right: 36rpx;bottom: 124rpx;
  width: 540rpx;height: 220rpx;
}
button{padding: 0;border-radius: 0;background: transparent}
.button-hover{background: transparent;}
button::after{border: none}
.operate-btn image{display: block;width: 100%;height: 100%;}


/* component/navbar/navbar.wxss */
/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */
.btns-cont{
  display: flex; justify-content: space-around;flex-direction: column
}
.nav-wrap {
  position: fixed;width: 100%;top: 0;background: transparent;color: #000;z-index: 999;
}
/* 标题要居中 */
.nav-title {
  position: absolute;text-align: center;
  max-width: 410rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;
  top: 0;left: 0;right: 0;bottom: 0;
  margin: auto;font-size: 36rpx;color: #fff;
}

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  min-width: 140rpx;
  height: 100%;
}
.navbar-btns-box{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 60rpx;width: 160rpx;
  /* padding: 0 24rpx; */
  background: rgba(0, 0, 0, 0.15);
  border: 1rpx solid rgba(225, 225, 225, 0.15);
  border-radius: 30rpx;
  position: relative;
}
.split-line{
  position: absolute;left: 50%;top:16rpx;
  width: 2rpx;height: 32rpx;
  background: rgba(225, 225, 225, 0.15);
}
.no-home-btn{
  background: transparent;border: none;
}
.idx-search-container{
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  height: 60rpx;width: 460rpx;
  border-radius: 30rpx;
  background: rgba(0, 0, 0, 0.15);
  position: relative;
}
.s-split-line{
  position: absolute;left: 72rpx;top:14rpx;
  width: 2rpx;height: 32rpx;
  background: rgba(225, 225, 225, 0.15);
}
.navbar-v-line {
  width: 1px;
  height: 32rpx;
  background-color: #e5e5e5;
}
.back-pre{
  width: 34rpx;height: 34rpx;margin-right: 56rpx;margin-left: 22rpx;
}
.back-home{
  width: 34rpx;height: 34rpx;margin-right: 22rpx;
}
